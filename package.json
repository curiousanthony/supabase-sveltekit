{
	"name": "supabase-sveltekit",
	"private": true,
	"version": "1.7.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"db:generate": "drizzle-kit generate",
		"db:pull": "drizzle-kit pull --config drizzle.pull.config.ts",
		"db:push": "echo 'Error: Use supabase db push instead' && exit 1",
		"db:migrate": "echo 'Error: Use supabase migration up (local) or supabase db push (remote) instead' && exit 1"
	},
	"devDependencies": {
		"@dnd-kit-svelte/svelte": "^0.1.3",
		"@dnd-kit/abstract": "^0.1.21",
		"@dnd-kit/helpers": "^0.1.21",
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.36.0",
		"@internationalized/date": "^3.10.0",
		"@lucide/svelte": "^0.544.0",
		"@sveltejs/adapter-vercel": "^5.10.2",
		"@sveltejs/kit": "^2.43.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@tabler/icons-svelte": "^3.35.0",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.18",
		"@tailwindcss/vite": "^4.1.13",
		"@tanstack/table-core": "^8.20.5",
		"@types/d3-scale": "^4.0.8",
		"@types/d3-shape": "^3.1.7",
		"@types/node": "^20",
		"@types/pg": "^8.15.5",
		"bits-ui": "^2.14.4",
		"clsx": "^2.1.1",
		"d3-scale": "^4.0.2",
		"d3-shape": "^3.2.0",
		"drizzle-kit": "^0.31.5",
		"eslint": "^9.36.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"formsnap": "^2.0.1",
		"globals": "^16.4.0",
		"layerchart": "2.0.0-next.27",
		"paneforge": "^1.0.2",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.39.5",
		"svelte-check": "^4.3.2",
		"svelte-sonner": "^1.0.5",
		"sveltekit-superforms": "^2.13.1",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.13",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.44.1",
		"vaul-svelte": "^1.0.0-next.7",
		"vite": "^7.1.7"
	},
	"dependencies": {
		"@supabase/ssr": "^0.7.0",
		"@supabase/supabase-js": "^2.75.1",
		"debug": "^4.4.3",
		"drizzle-orm": "^0.44.6",
		"mode-watcher": "^1.1.0",
		"postgres": "^3.4.7",
		"zod": "^3.25.64",
		"zod-form-data": "^3.0.1"
	},
	"optionalDependencies": {
		"@rollup/rollup-darwin-arm64": "4.34.9",
		"@rollup/rollup-darwin-x64": "4.34.9"
	},
	"release": {
		"branches": [
			"main"
		],
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			[
				"@semantic-release/npm",
				{
					"npmPublish": false
				}
			],
			[
				"@semantic-release/git",
				{
					"assets": [
						"package.json",
						"CHANGELOG.md"
					],
					"message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
				}
			],
			"@semantic-release/github"
		]
	}
}
